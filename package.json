{
	"name": "my-app",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"start": "vite preview --host",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"studio": "drizzle-kit studio --config ./src/drizzle.config.ts",
		"generate": "drizzle-kit generate:pg --config ./src/drizzle.config.ts",
		"migrate": "drizzle-kit push:pg --config ./src/drizzle.config.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.42.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.5.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@types/eslint": "8.56.5",
		"@typescript-eslint/eslint-plugin": "^7.1.0",
		"@typescript-eslint/parser": "^7.1.0",
		"autoprefixer": "^10.4.18",
		"drizzle-kit": "^0.20.14",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"instascan": "^1.0.0",
		"jsqr": "^1.4.0",
		"pg": "^8.11.3",
		"postcss": "^8.4.35",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.2",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.6",
		"tailwindcss": "^3.4.1",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3",
		"vite": "^5.1.4",
		"vitest": "^1.3.1"
	},
	"type": "module",
	"dependencies": {
		"bits-ui": "^0.18.6",
		"clsx": "^2.1.0",
		"dotenv": "^16.4.5",
		"drizzle-orm": "^0.29.4",
		"embla-carousel-svelte": "8.0.0",
		"formsnap": "^0.4.3",
		"html5-qrcode": "^2.3.8",
		"lucide-svelte": "^0.344.0",
		"postgres": "^3.4.3",
		"radix-icons-svelte": "^1.2.1",
		"svelte-material-ui": "7.0.0-beta.16",
		"svelte-routing": "^2.12.0",
		"svelte-sonner": "^0.3.19",
		"sveltekit-superforms": "^1.13.4",
		"tailwind-merge": "^2.2.1",
		"tailwind-variants": "^0.2.0",
		"zod": "^3.22.4"
	}
}